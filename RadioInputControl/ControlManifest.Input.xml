<?xml version="1.0" encoding="utf-8"?>
<manifest>
    <control namespace="RadioKAP"
             constructor="RadioInputControl"
             version="0.0.1"
             display-name-key="Radio Input Control"
             description-key="Radio Input Control Desc"
             control-type="standard">
        <external-service-usage enabled="false" />
        <type-group name="numbers">
            <type>Whole.None</type>
        </type-group>
        <property name="controlValue"
                  display-name-key="Radio Value"
                  description-key="Yes / No Control Value"
                  of-type="numbers"
                  usage="bound"
                  required="true" />
        <resources>
            <code path="index.ts"
                  order="1" />
            <css path="css/index.css"
                 order="1" />
        </resources>
    </control>
</manifest>